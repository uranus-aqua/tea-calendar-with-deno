{
  "name": "tea-calendar",
  "private": true,
  "version": "1.0.0",
  "description": "Convert Gregorian calendar date (1583-2100) to Chinese calendar date",
  "exports": {
    ".": {
      "import": "./dist/teac.mjs",
      "require": "./dist/teac.cjs"
    }
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/teac.cjs",
  "module": "./src/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "nyc --reporter=html --reporter=text mocha './test/*.test.*js' --reporter doc --timeout=12000"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "vite": "^4.4.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uranus-aqua/TEA-calendar.git"
  },
  "keywords": [
    "calendar",
    "conversion",
    "Gregorian",
    "Chinese"
  ],
  "author": "KUO Wei-Shiung",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uranus-aqua/TEA-calendar/issues"
  },
  "homepage": "https://github.com/uranus-aqua/TEA-calendar#readme",
  "dependencies": {
    "debug": "^4.3.4",
    "esm": "^3.2.25"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  }
